	<panel name="commonplatformNewsQueryPanel" type="formPanel"
		locateMode="flow"
		dataFormClassName="org.octopus.common_business.news.form.NewsInfoQueryForm"
		handlerClassName="cn.edu.sdu.uims.handler.impl.UInnerHandler">
		<components>
			<component name="newsTypeLabel" type="label" border="fnull" horizontalAlignment ="right"
				x="0" y="10" w="100" h="25" text="新闻类型"  >
			</component>
			<component name="newsType" type="comboBoxDict" border="f1" dictionary= "XWLXM"
				x="110"	y="10" w="150" h="25" dataFormMember="newsType" addSelectItem ="true">
			</component>
			<component name="newsNumLabel" type="label" border="fnull" horizontalAlignment ="right"
				x="0" y="50" w="100" h="25" text="消息编号"  >
			</component>
			<component name="newsNum" type="textField" border="f1" horizontalAlignment ="left" maxLength = "15"  
				x="110"	y="50" w="150" h="25" dataFormMember="newsNum" >
			</component>
			<component name="titleLabel" type="label" border="fnull" horizontalAlignment ="right"
				x="0" y="50" w="100" h="25" text="消息标题"  >
			</component>
			<component name="title" type="textField" border="f1" horizontalAlignment ="left"  
				x="110"	y="50" w="150" h="25" dataFormMember="title" maxLength = "15" >
			</component>
			<component name="statetDateLabel" type="label" border="fnull" horizontalAlignment ="right"
				x="0" y="50" w="100" h="25" text="起止时间"  >
			</component>
			<component name="statetDate" type="comboBoxDate" border="f1" horizontalAlignment ="left"  
				x="110"	y="50" w="150" h="25" dataFormMember="statetDate" maxLength = "15"  defaultCurrentDate ="false">
			</component>
			<component name="endDate" type="comboBoxDate" border="f1" horizontalAlignment ="left"  
				x="110"	y="50" w="150" h="25" dataFormMember="statetDate" maxLength = "15" defaultCurrentDate ="false" >
			</component>
			<component name="isVisable" type="checkBox" border="f1" horizontalAlignment ="left"  text ="新闻是否显示"
				x="110"	y="50" w="150" h="25" dataFormMember="isVisable" maxLength = "15" >
			</component>
			<component name="query" type="button" border="f1" horizontalAlignment ="center"  
				x="110"	y="50" w="80" h="25"  text="查询" >
				<event type="action" />				
			</component>
		</components>
	</panel>
	 
	<panel name="commonplatformNewsInfoPanel" type="formPanel"
		locateMode="locate" 
		dataFormClassName = "org.octopus.common_business.news.form.NewsInfoForm"
		handlerClassName="cn.edu.sdu.commonplatform.handler.NewsInfoInfoHandler"
		 width = "1000" height = "90" >
		<components>
			<component name="titleLabel" type="label" text="标题"  horizontalAlignment= "right"
				font="formFont" border="fnull" x="0" y="10" w="60" h="30" />
			<component name="title" type="textField" text="" dataFormMember ="title" 
				font="formFont" border="f1" x="70" y="10" w="430" h="30" >
			</component>
			<component name="briefLabel" type="label" text="简述"  horizontalAlignment= "right"
				font="formFont" border="fnull" x="0" y="50" w="60" h="30" />
			<component name="brief" type="textArea" text="" dataFormMember ="brief" 
				font="formFont" border="f1" x="70" y="50" w="430" h="150" >
			</component>
			<component name="URLLabel" type="label" text="URL"  horizontalAlignment= "right"
				font="formFont" border="fnull" x="0" y="210" w="60" h="30" />
			<component name="URL" type="textField" text="" dataFormMember ="URL" editable = "false"
				font="formFont" border="f1" x="70" y="210" w="430" h="30" >
				<event type="mouse" />
			</component>
		
			<component name="newTypeLabel" type="label" text="通知类型" horizontalAlignment= "right"
				font="formFont" border="fnull" x="510" y="10" w="90" h="30" />
			<component name="newsType" type="comboBoxDict" dataFormMember="newsType" dictionary="XWLXM"
				font="formFont"  x="610" y="10" w="100" h="30" addSelectItem ="true" horizontalAlignment="left"    >
			</component>
			<component name="newsNumLabel" type="label" text="新闻编号"  horizontalAlignment= "right"
				font="formFont" border="fnull" x="510" y="50" w="90" h="30" />
			<component name="newsNum" type="textField" text="" dataFormMember ="newsNum" editable = "false"
				font="formFont" border="f1" x="610" y="50" w="100" h="30" />
			<component name="orderNumLabel" type="label" text="指定新闻顺序" horizontalAlignment= "right"
				font="formFont" border="fnull" x="510" y="90" w="90" h="30" />
			<component name="orderNum" type="textField" text="" dataFormMember ="orderNum"
				font="formFont" border="f1" x="610" y="90" w="100" h="30" />
			<component name="createTimeLabel" type="label" text="创建时间" horizontalAlignment= "right"
				font="formFont" border="fnull" x="510" y="130" w="90" h="30" />
			<component name="createTime" type="comboBoxDate"  text=""	font="formFont" 
				border="f1" x="610" y="130" w="100" h="30"  defaultCurrentDate= "false"
				dataFormMember ="createTime"/>
			<component name="createrNameLabel" type="label" text="创建者" horizontalAlignment= "right"
				font="formFont" border="fnull" x="510" y="170" w="90" h="30" />
			<component name="createrName" type="textField" text="" dataFormMember ="createrName" editable = "false"
				font="formFont" border="f1" x="610" y="170" w="100" h="30" />
			<component name="personSearch" type="button" text="查询" horizontalAlignment= "center"
				font="formFont" border="f1" x="720" y="170" w="100" h="30" >
				<event type="action" />
			</component>
			<component name="isVisable" type="checkBox01I" text="新闻是否可见" dataFormMember ="isVisable"
				font="formFont" border="f1" x="610" y="210" w="100" h="30" />
			<component name="attachId" type="imageAttach"  dataFormMember ="attachId"
				font="formFont" border="f1" x="720" y="10" w="100" h="100" />

			<component name="createButton" type="button" text="新建" horizontalAlignment= "center"
				font="formFont" border="f1" x="150" y="250" w="90" h="30" >
				<event type="action" />				
			</component>
			<component name="saveButton" type="button" text="保存" horizontalAlignment= "center"
				font="formFont" border="f1" x="250" y="250" w="90" h="30" >
				<event type="action" />				
			</component>
			<component name="deleteButton" type="button" text="删除"  horizontalAlignment= "center"
				font="formFont" border="f1" x="350" y="250" w="90" h="30" >
				<event type="action" />				
			</component>
			<component name="uploadImage" type="button" text="照片上传"  horizontalAlignment = "center"
				font="formFont" border="f1" x="450" y="250" w="90" h="30" >
				<event type="action" />
			</component>
			<component name="uploadHtml" type="button" text="网页上传"  horizontalAlignment = "center"
				font="formFont" border="f1" x="550" y="250" w="90" h="30" >
				<event type="action" />
			</component>
		</components>
	</panel>
	<panel name="commonplatformNewsEditPanel" type="formPanel"
		locateMode="border" 
		dataFormClassName="cn.edu.sdu.uims.form.impl.UTableForm"
		handlerClassName="cn.edu.sdu.uims.handler.impl.UInnerHandler">
