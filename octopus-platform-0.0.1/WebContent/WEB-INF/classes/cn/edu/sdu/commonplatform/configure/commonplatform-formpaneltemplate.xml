<?xml version="1.0" encoding="UTF-8"?>
<formpaneltemplate>
    <panel name="commonplatformUserPasswordChangePanel" type="formPanel"
		locateMode="locate"
		dataFormClassName="cn.edu.sdu.uims.form.impl.PasswdForm"
		handlerClassName="cn.edu.sdu.commonplatform.handler.UserChangePasswordHandler">
		<components>
			<component name="oldPwdLabel" type="label" horizontalAlignment = "right" text ="旧密码"  enLabel ="CLAVE ANTERIOR"
				border="fnull" font="formFont" x="10" y="10" w="100" h="30">
			</component>
		    <component name="enterOldPwd" type="passwordField"  dataFormMember="enterOldPwd"
				border="f1" font="formFont" x="110" y="10" w="150" h="30">
			</component>
			<component name="newPwdLabel" type="label" horizontalAlignment = "right" text ="新密码"  enLabel ="CLAVE NUEVA"
				border="fnull" font="formFont" x="10" y="60" w="100" h="30">
			</component>
		    <component name="enterNewPwd" type="passwordField"  dataFormMember="enterNewPwd"
				border="f1" font="formFont" x="110" y="60" w="150" h="30">
			</component>
			<component name="conPwdLabel" type="label" horizontalAlignment = "right" text ="确认密码" enLabel ="REPETIR CLAVE"
				border="fnull" font="formFont" x="10" y="110" w="100" h="30">
			</component>
		    <component name="enterRepeatNewPwd" type="passwordField"  dataFormMember="enterRepeatNewPwd"
				border="f1" font="formFont" x="110" y="110" w="150" h="30">
			</component>
		    <component name="ok" type="button" border="f1"   horizontalAlignment = "center"
				font="formFont" text="确认" x="120" y="160" w="100" h="30"  enLabel ="ACEPTAR" >
				<event type="action" />
			</component>
		</components>
	</panel>   
