			</component>
		</components>
	</panel>    		
	
    <panel name="commonplatformUserHelpInfoQueryPanel"	type="formPanel" className="cn.edu.sdu.uims.component.panel.UFPanel"
		locateMode="flow" text=""	
		dataFormClassName="org.octopus.common_business.help.form.UserHelpInfoForm"
		handlerClassName="cn.edu.sdu.uims.handler.impl.UInnerHandler">
		<components>
			<component name="perTypeLable" type="label" border="fnull" horizontalAlignment="right"
				x="10" y="10" w="100" h="25" text="商户类型" enLabel = "TIPO DE USUARIO" >
			</component>
			<component name="perType" type="comboBoxDict" border="f1"  dictionary = "JYSHLXM"
				x="110"	y="10" w="150" h="25" dataFormMember="perType"  >
		        <event type = "action" />
			</component>
			<component name="studyLevelLabel" type="label" border="fnull" horizontalAlignment="right"
				x="10" y="10" w="100" h="25" text="学习级别" enLabel = "PAGINA" >
			</component>
			<component name="studyLevel" type="comboBoxType" border="f1"  
				x="110"	y="10" w="150" h="25" dataFormMember="studyLevel" filter="uims_common_filter"  >
		        <event type = "action" />
			</component>
			<component name="doQuery" type="button" border="fnull" horizontalAlignment="center"
				x="10" y="10" w="100" h="25" text="查询" enLabel = "CONSULTA" >
		        <event type = "action" />
			</component>
			<component name="doModify" type="button" border="fnull" horizontalAlignment="center"
				x="10" y="10" w="150" h="25" text="修改" enLabel = "GUARDAR"  cmd="doModify">
		        <event type = "action" />
			</component>
			<component name="doAdd" type="button" border="fnull" horizontalAlignment="center"
				x="10" y="10" w="150" h="25" text="添加" enLabel = "AGREGAR PAGINA"  cmd="doAdd">
		        <event type = "action" />
			</component>
		</components>
	</panel>
	
