			</component>
		</components>
	</panel>    		
	<panel name="commonplatformNewsManagePanel" type="formPanel"
		locateMode="border" 
		dataFormClassName="cn.edu.sdu.uims.form.impl.UTableForm"
		handlerClassName="cn.edu.sdu.commonplatform.handler.NewsInfoManageHandler">
